* {
    background-color: transparent;
}

window {
    background-color: #0f0f12;
    border-color: #9298c9;
}

prompt, entry, textbox-prompt-colon {
    text-color: #9298c9;
}

listview {
    border-color: #9298c9;
    border: 0px 0px;
}

scrollbar {
    border: 0;
    handle-color: #9298c9;
}

element normal normal {
    text-color: #9298c9;
    background-color: #0f0f12;
}

element selected normal {
    text-color: #0f0f12;
    background-color: #9298c9;
}

element alternate normal {
    text-color: #9298c9;
    background-color: #0f0f12;
}
